<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>Diamond's Laravel helpers</title>
    <parser>
        <target>build</target>
    </parser>
    <transformer>
        <target>docs</target>
    </transformer>
    <transformations>
        <!-- lets us know if there are any errors in our comments -->
        <template name="checkstyle" />
        <template name="../data/templates/dimezilla-markdown" />
    </transformations>
    <files>
        <directory>src/</directory>
    </files>
</phpdoc>
